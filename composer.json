{
    "name": "lcobucci/behat-di-builder-extension",
    "description": "A behat extension that allows injecting services from a container created with lcobucci/di-builder in contexts",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "behat/behat": "^3.8",
        "lcobucci/di-builder": "^7.0"
    },
    "require-dev": {
        "lcobucci/coding-standard": "^6.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\Behat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\Behat\\Tests\\": "tests"
        }
    }
}
