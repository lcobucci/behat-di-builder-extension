{
    "name": "lcobucci/behat-di-builder-extension",
    "description": "A behat extension that allows injecting services from a container created with lcobucci/di-builder in contexts",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "behat/behat": "^3.12.0",
        "lcobucci/di-builder": "^7.2.1"
    },
    "require-dev": {
        "lcobucci/coding-standard": "^11.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.10.7",
        "phpstan/phpstan-deprecation-rules": "^1.1.3",
        "phpstan/phpstan-phpunit": "^1.3.10",
        "phpstan/phpstan-strict-rules": "^1.5.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\Behat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lcobucci\\DependencyInjection\\Behat\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
